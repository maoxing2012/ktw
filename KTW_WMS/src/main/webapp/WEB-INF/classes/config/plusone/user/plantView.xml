				<text id="plant.name" title="plant.name" width="335" span="3"/>
				<textArea id="plant.description" title="title.desc" rows="50" cols="380"  span="3"/>
			</parttab>
			<parttab id="tab_2" title="title.contractInfo" style="CTab" tabHeight="300" >
