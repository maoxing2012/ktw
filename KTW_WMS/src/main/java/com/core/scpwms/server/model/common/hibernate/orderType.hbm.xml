<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.core.scpwms.server.model.common.OrderType"  table="WMS_ORDER_TYPE">
		<id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">wseq_ordertype</param>
                <param name="parameters">START WITH 1000</param>
            </generator>
        </id>
        <discriminator column="DISCRIMINATOR" type="string"/>
		<property name="code" column="CODE" type="string" not-null="true" unique-key="UK_ORDER_TYPE" length="50"/>
		<property name="name" column="NAME" type="string" not-null="true"/>
		<property name="processType" column="PROCESS_TYPE" type="string" length="50"/>
        <property name="systemType" column="SYSTEM_TYPE" type="boolean"/>
        <property name="urnoverCount" column="URNOVER_COUNT" type="boolean"/>
        <property name="preAllocate" column="PRE_ALLOCATE" type="boolean"/>
        
        <many-to-one name="seqProperties" class="com.core.scpwms.server.model.common.SequenceProperties">
            <column name="SEQ_PROPERTIES_ID"/>
        </many-to-one>
        
        <property name="disabled" column="DISABLED" type="boolean"/>
        <property name="description" column="DESCRIPTION" type="string"/>
		<component name="createInfo" class="com.core.business.model.domain.CreateInfo">
			<property name="operatorId" column="CREATE_OPERATOR_ID" type="long" />
			<property name="operatorName" column="CREATE_OPERATOR_NAME" type="string" length="50"/>
			<property name="updateTime" column="CREATE_TIME" type="timestamp" />
		</component>	
		<component name="updateInfo" class="com.core.business.model.domain.UpdateInfo">
	       	<property name="operatorId" column="UPDATE_OPERATOR_ID" type="long" />
	        <property name="operatorName" column="UPDATE_OPERATOR_NAME" type="string" length="50"/>
	        <property name="updateTime" column="UPDATE_TIME" type="timestamp" />
        </component>
	</class>
</hibernate-mapping>