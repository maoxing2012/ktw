<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.core.scpwms.server.model.transport.PostCode" table="WMS_POST_CODE">
    	<id name="id" column="ID" type="long">
    		<generator class="native">
    			<param name="sequence">wseq_postcode</param>
    			<param name="parametes">START WITH 1000</param>
    		</generator>
    	</id>
    	
    	<many-to-one name="city" class="com.core.scpwms.server.model.transport.City">
	 		<column name="CITY_ID" />
	 	</many-to-one>
    	 	
    	<property name="addr" column="ADDR" type="string" length="255"/>
    	<property name="postcode" column="POST_CODE" type="string" length="50" unique="true"/>
    </class>
</hibernate-mapping>


